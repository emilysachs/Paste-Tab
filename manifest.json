{
  "manifest_version": 2,
  "name": "Paste Query",
  "author": "Emily Sachs",
  "version": "1.0",
  "icons": { "16": "PasteQuery16.png",
             "48": "PasteQuery48.png",
            "128": "PasteQuery.png" },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["paste.js"]
  }],
  "background": {
    "scripts": ["query.js"],
    "persistent": false
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "commands": {
	  "paste-tab": {
	    "suggested_key": {
	      "default": "Ctrl+Shift+E",
	      "mac": "Command+Shift+E"
	    },
	    "description": "Search from clipboard"
	  }
	}
}